from flask import Flask, render_template, jsonify
import json

app = Flask(__name__)

@app.route("/")
def index():
    with open("jfrog_permissions.json") as f:
        data = json.load(f)
    return render_template("index.html", data=data)

@app.route("/api/data")
def api_data():
    with open("jfrog_permissions.json") as f:
        data = json.load(f)
    return jsonify(data)

if __name__ == "__main__":
    app.run(host="0.0.0.0", port=5000, debug=False)
